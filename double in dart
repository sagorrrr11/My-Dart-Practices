void main (){
  // <datatype> <variablename> = value;
  double firstvalue = 44.45;
  double secondvalue = 46.64;
  double thirdvalue = 43.44;
  print(firstvalue);
  print(secondvalue);
  print(thirdvalue);
  print(firstvalue*secondvalue);
  print(firstvalue/thirdvalue);
  print(firstvalue+secondvalue);
  print(thirdvalue-secondvalue);
}
