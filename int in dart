void main (){
  // variables
  // <datatybe> <variable> - value;
  int firstvalue = 44;
  int secondvalue = 46;
  int thirdvalue = 43;
  print(firstvalue);
  print(secondvalue);
  print(thirdvalue);
  print(firstvalue*secondvalue);
  print(firstvalue/thirdvalue);
  print(firstvalue+secondvalue);
  print(thirdvalue-secondvalue);
}
