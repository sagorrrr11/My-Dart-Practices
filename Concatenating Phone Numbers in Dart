void main() {
  List<String> phoneNumber = [
    '+88',
    '01707495380',
    '01789619711',
    '01761089281',
    '01749237361'
  ];

  print("The list of numbers is: "
      "${phoneNumber[0]}${phoneNumber[1]},"
      "${phoneNumber[0]}${phoneNumber[2]},"
      "${phoneNumber[0]}${phoneNumber[3]},"
      "${phoneNumber[0]}${phoneNumber[4]}.");
}
