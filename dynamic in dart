void main (){
  // variables
  // <datatype> <variablename> = value;
  dynamic firstDataType = 58;
  print(firstDataType);
  dynamic secondDataType = 45.58;
  print (secondDataType);
  dynamic thirdDataType = 'Hello';
  print(thirdDataType);
  dynamic fourthDataType = false;
  print(fourthDataType);
}
