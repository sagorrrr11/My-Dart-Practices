void main (){
  // variables
  // var/final/const <variablename> = value;
  var somevalue1 = 10;
  final somevalue2 = 110;
  const somevalue3 = 140;
  
  print(somevalue1);
  print(somevalue2);
  print(somevalue3);
  
  somevalue1 = 2000;
  somevalue2 = 3000; //error
  somevalue3 = 4999; //error
  
  print(somevalue1);
  print(somevalue2);
  print(somevalue3);
}
